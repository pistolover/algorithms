		<!-- 源文件编码 -->
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- 插件版本 -->
		<maven-compiler-plugin>2.5.1</maven-compiler-plugin>

		<!-- 依赖 -->
		<javax.servlet-api>3.1.0</javax.servlet-api>
